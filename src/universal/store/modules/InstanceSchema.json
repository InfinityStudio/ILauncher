{
    "type": "object",
    "properties": {
        "id": {
            "description": "The unique id (uuid) of the profile. The profile data will be stored into profiles/${id}/profile.json according to this.",
            "default": "",
            "type": "string"
        },
        "name": {
            "description": "The display name of the profile. It will also be the modpack display name",
            "default": "",
            "type": "string"
        },
        "author": {
            "description": "The author of this instance",
            "type": "string"
        },
        "description": {
            "description": "The description of this instance",
            "type": "string"
        },
        "showLog": {
            "description": "Should show a logger window after Minecraft launched",
            "default": false,
            "type": "boolean"
        },
        "hideLauncher": {
            "description": "Should launcher hide after Minecraft launched",
            "default": true,
            "type": "boolean"
        },
        "deployments": {
            "description": "The external resource deployment of this instance, like mods or resource packs.\n\nThe domain is the directory it will deployed to. For example, `mods` will deploy to `.minecraft/mods` folder.\n\nEach is a list of uri in specific format\n\n- Curseforge file `curseforge://projectId/fileId` or `curseforge://fileId`\n- Forge mod: `forge://modid/version`\n- Liteloader mod: `liteloader://name/version`\n- Fabric mod: `fabric://name/version`\n- Local managed resource: `resource://hash`\n- Http: `https://abc.bcd.jar`\n- Github: `github://username/reponame/releaseName`",
            "default": {},
            "type": "object",
            "additionalProperties": {
                "type": "array",
                "items": {
                    "type": "string"
                }
            }
        },
        "optionalDeployments": {
            "description": "The optional external resource deployment of this profiles, like mods or resource packs",
            "default": [],
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "runtime": {
            "description": "The runtime version requirement of the profile.\n\nContaining the forge & liteloader & etc.",
            "default": {
                "minecraft": "",
                "forge": "",
                "liteloader": ""
            },
            "type": "object",
            "additionalProperties": {
                "type": "string"
            },
            "properties": {
                "minecraft": {
                    "default": "",
                    "type": "string"
                },
                "forge": {
                    "default": "",
                    "type": "string"
                },
                "liteloader": {
                    "default": "",
                    "type": "string"
                },
                "fabric": {
                    "default": "",
                    "type": "string"
                }
            },
            "required": [
                "forge",
                "liteloader",
                "minecraft"
            ]
        },
        "java": {
            "description": "The recommended java version for this instance",
            "default": "8",
            "type": "string"
        },
        "resolution": {
            "type": "object",
            "properties": {
                "width": {
                    "type": "number"
                },
                "height": {
                    "type": "number"
                },
                "fullscreen": {
                    "type": "boolean"
                }
            },
            "required": [
                "fullscreen",
                "height",
                "width"
            ]
        },
        "minMemory": {
            "type": "number"
        },
        "maxMemory": {
            "type": "number"
        },
        "vmOptions": {
            "default": [],
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "mcOptions": {
            "default": [],
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "url": {
            "default": "",
            "type": "string"
        },
        "icon": {
            "default": "",
            "type": "string"
        },
        "image": {
            "default": null,
            "type": [
                "null",
                "string"
            ]
        },
        "blur": {
            "default": 0,
            "type": "number"
        },
        "lastAccessDate": {
            "default": 0,
            "type": "number"
        },
        "creationDate": {
            "default": 0,
            "type": "number"
        },
        "server": {
            "description": "The option for instance to launch server directly",
            "type": "object",
            "properties": {
                "host": {
                    "type": "string"
                },
                "port": {
                    "type": "number"
                }
            },
            "required": [
                "host"
            ]
        },
        "licence": {
            "type": "string"
        }
    },
    "required": [
        "blur",
        "creationDate",
        "deployments",
        "hideLauncher",
        "icon",
        "id",
        "image",
        "java",
        "lastAccessDate",
        "mcOptions",
        "name",
        "optionalDeployments",
        "runtime",
        "showLog",
        "url",
        "vmOptions"
    ],
    "$schema": "http://json-schema.org/draft-07/schema#",
    "additionalProperties": false
}