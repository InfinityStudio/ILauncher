{
    "type": "object",
    "properties": {
        "profiles": {
            "description": "All saved user profiles",
            "default": {},
            "type": "object",
            "additionalProperties": {
                "$ref": "#/definitions/UserProfile"
            }
        },
        "selectedUser": {
            "default": "",
            "type": "string"
        },
        "selectedUserProfile": {
            "default": "",
            "type": "string"
        },
        "authServices": {
            "description": "All loaded auth services api. Used for ygg auth",
            "default": {},
            "type": "object",
            "additionalProperties": {
                "$ref": "#/definitions/Auth.Yggdrasil.API"
            }
        },
        "profileServices": {
            "description": "All loaded profile services api. Used for",
            "default": {},
            "type": "object",
            "additionalProperties": {
                "$ref": "#/definitions/ProfileService.API"
            }
        },
        "loginHistory": {
            "description": "The login history of the user for a specific auth services",
            "default": [],
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "clientToken": {
            "description": "The client token of current client. The launcher will generate one at first launch.",
            "default": "",
            "type": "string"
        }
    },
    "required": [
        "authServices",
        "clientToken",
        "loginHistory",
        "profileServices",
        "profiles",
        "selectedUser",
        "selectedUserProfile"
    ],
    "definitions": {
        "UserProfile": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "string"
                },
                "account": {
                    "description": "The account usually email",
                    "type": "string"
                },
                "profileService": {
                    "description": "The used profile service id",
                    "type": "string"
                },
                "authService": {
                    "description": "The used auth service id",
                    "type": "string"
                },
                "accessToken": {
                    "description": "The access token of the",
                    "type": "string"
                },
                "profiles": {
                    "description": "all avaiable profiles",
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/GameProfileAndTexture"
                    }
                }
            },
            "required": [
                "accessToken",
                "account",
                "authService",
                "id",
                "profileService",
                "profiles"
            ]
        },
        "GameProfileAndTexture": {
            "type": "object",
            "properties": {
                "textures": {
                    "type": "object",
                    "properties": {
                        "SKIN": {
                            "$ref": "#/definitions/GameProfile.Texture"
                        },
                        "CAPE": {
                            "description": "The data structure that hold the texture",
                            "$ref": "#/definitions/GameProfile.Texture"
                        },
                        "ELYTRA": {
                            "description": "The data structure that hold the texture",
                            "$ref": "#/definitions/GameProfile.Texture"
                        }
                    },
                    "required": [
                        "SKIN"
                    ]
                },
                "id": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "properties": {
                    "type": "object",
                    "additionalProperties": {
                        "type": "string"
                    }
                }
            },
            "required": [
                "id",
                "name",
                "textures"
            ]
        },
        "GameProfile.Texture": {
            "description": "The data structure that hold the texture",
            "type": "object",
            "properties": {
                "url": {
                    "type": "string"
                },
                "metadata": {
                    "type": "object",
                    "additionalProperties": {},
                    "properties": {
                        "model": {
                            "enum": [
                                "slim",
                                "steve"
                            ],
                            "type": "string"
                        }
                    }
                }
            },
            "required": [
                "url"
            ]
        },
        "Auth.Yggdrasil.API": {
            "type": "object",
            "properties": {
                "hostName": {
                    "description": "The host url, like https://xxx.xxx.com",
                    "type": "string"
                },
                "authenticate": {
                    "description": "Authenticate path, in the form of `/your-endpoint`.\nUse to login",
                    "type": "string"
                },
                "refresh": {
                    "description": "Use to refresh access token",
                    "type": "string"
                },
                "validate": {
                    "description": "Use to validate the user access token",
                    "type": "string"
                },
                "invalidate": {
                    "description": "Use to logout user (invalidate user access token)",
                    "type": "string"
                },
                "signout": {
                    "description": "Use to logout user (by username and password)",
                    "type": "string"
                }
            },
            "required": [
                "authenticate",
                "hostName",
                "invalidate",
                "refresh",
                "signout",
                "validate"
            ]
        },
        "ProfileService.API": {
            "type": "object",
            "properties": {
                "publicKey": {
                    "description": "The PEM public key",
                    "type": "string"
                },
                "profile": {
                    "description": "Full url to query profile by uuid. Place the uuid as `${uuid}` in this url",
                    "type": "string"
                },
                "profileByName": {
                    "description": "Full url to query profile by name. Place the name as `${name}` in this url",
                    "type": "string"
                },
                "texture": {
                    "description": "Full url to set texture by profile uuid and texture type. Place uuid as `${uuid}` and type as `${type}`",
                    "type": "string"
                }
            },
            "required": [
                "profile",
                "profileByName",
                "texture"
            ]
        }
    },
    "$schema": "http://json-schema.org/draft-07/schema#",
    "additionalProperties": false
}